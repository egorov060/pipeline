pipeline {
    agent {
    any {
        image 'node:192.168.55.22:8123/jenkins-maven' 
        args '-u root'
    }
}
stages {
    stage ('Copy source') {
        steps {
            git 'https://github.com/egorov060/origin555.git'
        }
    }
    stage ('build war') {
        steps {
            sh 'mvn package'
        }
    }
    stage ('Make docker image') {
        steps {
            sh 'docker build -t myweb .' 
            sh 'docker tag myweb 192.168.55.22:8123/myweb $$ docker push 192.168.55.22:8123/myweb'
        }
    }
}
}